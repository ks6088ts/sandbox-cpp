{
	"name": "sandbox-cpp",
	"dockerComposeFile": [
		"../docker-compose.yml",
	],
	"appPort": [
		8080,
	],
	"service": "cpp",
	"workspaceFolder": "/workspace",
	"shutdownAction": "stopCompose",
	"settings": {
		"editor.formatOnSave": true,
	},
	"extensions": [
		"EditorConfig.EditorConfig",
		"ms-vscode.cpptools",
		"ms-vscode.cmake-tools",
		"twxs.cmake",
	]
}
